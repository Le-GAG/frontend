{
  "presets": [
      ["env", {
          "modules": false,
          "targets": {
              "browsers": [
                ">= 1%",
                "not ie < 11",
                "not op_mini all"
              ]
          }
      }]
  ],
  "plugins": ["transform-runtime"],
  "comments": false,
  "env": {
    "test": {
      "plugins": [ "istanbul" ]
    }
  }
}
